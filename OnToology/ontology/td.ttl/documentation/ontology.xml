<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.w3.org/ns/td#"
     xml:base="http://www.w3.org/ns/td"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:ns="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://www.w3.org/ns/td#">
        <dc:publisher rdf:resource="https://www.w3.org/WoT/WG/"/>
        <dc:title xml:lang="en">Ontology model for Web of Things</dc:title>
        <terms:contributor rdf:resource="http://www.oeg-upm.net/index.php/en/universitystaff/402-fserena/"/>
        <terms:creator rdf:resource="http://purl.org/net/mpoveda"/>
        <terms:creator rdf:resource="http://www.garcia-castro.com/foaf.rdf#me"/>
        <terms:license rdf:resource="http://purl.org/NET/rdflicense/cc-by4.0"/>
        <vann:preferredNamespacePrefix rdf:datatype="http://www.w3.org/2001/XMLSchema#string">td</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.w3.org/ns/td#</vann:preferredNamespaceUri>
        <rdfs:comment xml:lang="en">This ontology aims to model the Web of Things domain according to the W3C Interest Group (http://w3c.github.io/wot/)</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.0.1</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2003/06/sw-vocab-status/ns#term_status -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2003/06/sw-vocab-status/ns#term_status"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/ns/td#actions -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/td#actions">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/td#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/td#Action"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Keeps all Action-based interaction pattern of the Thing.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">actions</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/td#events -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/td#events">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Keeps all Event-based interaction pattern of the Thing.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">events</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/td#forms -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/td#forms">
        <rdfs:comment xml:lang="en">Indicates one or more endpoints from which an interaction pattern is accessible</rdfs:comment>
        <rdfs:label xml:lang="en">forms</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/td#input -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/td#input">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/td#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/td#Property"/>
        <rdfs:comment xml:lang="en">link to the n-ary class that allows the declaration of the accepted data type of an action.</rdfs:comment>
        <rdfs:label xml:lang="en">input</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/td#items -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/td#items">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/td#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/td#Property"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Used to defines characteristics of an array.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">items</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/td#links -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/td#links">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">provides Web links to arbitrary resources that relate to the specified Thing Description.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">links</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/td#objectSchemaProperties -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/td#objectSchemaProperties">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Data schema nested definitions</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">properties</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/td#output -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/td#output">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/td#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/td#Property"/>
        <rdfs:comment xml:lang="en">link to the n-ary class that allows the declaration of the data type returned by an action.</rdfs:comment>
        <rdfs:label xml:lang="en">output</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/td#properties -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/td#properties">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/td#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/td#Property"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Keeps all Property-based interaction pattern of the Thing.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">properties</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/td#schema -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/td#schema">
        <rdfs:range rdf:resource="http://www.w3.org/ns/td#Property"/>
        <rdfs:comment xml:lang="en">schema definition for values of the given object schema property.</rdfs:comment>
        <rdfs:label xml:lang="en">schema</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/td#securityDefinition -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/td#securityDefinition">
        <rdfs:comment xml:lang="en">Links a given Thing to the security information that indicates the access metadata information for securely transmitting information via all the resources of the Thing.</rdfs:comment>
        <rdfs:label xml:lang="en">securityDefinitions</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/ns/td#anchor -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#anchor">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">By default, the context of a link is the URL of the representation it is associated with, and is serialised as a URI. When present, the anchor parameter overrides this with another URI, such as a fragment of this resource, or a third resource (i.e., when the anchor value is an absolute URI).</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">anchor</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#base -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#base">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment xml:lang="en">Define base URI that is valid for all defined local interaction resources. All other URIs in the TD must then be resolved using the algorithm defined in [[!RFC3986]]. This field is optional.</rdfs:comment>
        <rdfs:label xml:lang="en">base</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#const -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#const">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Provides a constant value.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">const</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#description -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#description">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Provides additionel (human) readable information.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#enum -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#enum">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Restricted set of values provided as array.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">enum</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#href -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#href">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment xml:lang="en">URI of the endpoint where an interaction pattern is provided</rdfs:comment>
        <rdfs:label xml:lang="en">href</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#id -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#id">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines unique URI (includes also URNs) ID of the Thing.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">id</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#in -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#in">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies location of the authentication information</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">in</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#label -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#label">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Provides a label (e.g., display a text for UI representation) of the interaction pattern.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">label</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#maxItems -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#maxItems">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the maximum number of itmes that have to be in the array.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">maxItems</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#maximum -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#maximum">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a maximum numeric value. Only applicable for associated number or integer types.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">maximum</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://schema.org/maxValue"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#mediaType -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#mediaType">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Assign underlying media type of the interaction pattern based on IANA (https://www.iana.org/assignments/media-types/media-types.xhtml)</rdfs:comment>
        <rdfs:label xml:lang="en">mediaType</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#minItems -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#minItems">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the minimum number of itmes that have to be in the array.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">minItems</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#minimum -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#minimum">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Specifies a minimum numeric value. Only applicable for associated number or integer types.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">minimum</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://schema.org/minValue"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#name -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#name">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Name of the Thing or interaction pattern (must be a valid keyword to be used in the scripting API)</rdfs:comment>
        <rdfs:label xml:lang="en">name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#observable -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#observable">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Indicates whether a remote servient can subscribe to (&quot;observe&quot;) the Property, to receive change notifications or periodic updates (true/false). </rdfs:comment>
        <rdfs:label xml:lang="en">observable</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#rel -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#rel">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Indicates the expected result of performing the operation described by the form.

For example, the Property interaction allows get and set operations. The protocol binding may contain a form for the get operation and a different form for the set operation. The value of the &quot;rel&quot; attribute of the form indicates which form is which and allows the client to select the correct form for the oeration required</rdfs:comment>
        <rdfs:label xml:lang="en">rel</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#required -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#required">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines which members of the object type are required/mandatory.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">required</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#scheme -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#scheme">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Identification of security scheme (e.g., basic, digest, and oauth2).</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">scheme</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#security -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#security">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Defines the security requirements of the interacions by making a reference to the top level securityDefinition</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">security</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#support -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#support">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Provides information about TD maintainer (e.g., author, link or telephon number to get support, etc).</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">support</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#type -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#type">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Assignment of JSON-based data types that are compatible with JSON Schema (boolean, integer, number, string, object, array, null).</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/td#writable -->


    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/td#writable">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">Boolean value that indicates whether a property is writable (=true) or not (=false)</rdfs:comment>
        <rdfs:label xml:lang="en">writable</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/ns/td#Action -->


    <owl:Class rdf:about="http://www.w3.org/ns/td#Action">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/td#forms"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/td#Form"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/td#input"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/td#Property"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/td#output"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/td#Property"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/td#description"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/td#label"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Actions offer functions of the Thing. These functions may manipulate the interal state of a Thing in a way that is not possible through setting Properties. Examples are changing internal state that is not exposed as Property, changing multiple Properties, changing Properties over time or with a process that shall not be disclosed. Actions may also be pure functions, that is, they do not use any internal state at all, e.g., for processing input data and returning the result directly.</rdfs:comment>
        <rdfs:label xml:lang="en">Action</rdfs:label>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/td#ArraySchema -->


    <owl:Class rdf:about="http://www.w3.org/ns/td#ArraySchema">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/td#DataSchema"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/td#items"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/td#DataSchema"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/td#maxItems"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/td#minItems"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedInt"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A JSON array specification (&quot;type&quot;: &quot;array&quot;).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/td#BooleanSchema -->


    <owl:Class rdf:about="http://www.w3.org/ns/td#BooleanSchema">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/td#DataSchema"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A JSON boolean value specification (&quot;type&quot;: &quot;boolean&quot;).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/td#DataSchema -->


    <owl:Class rdf:about="http://www.w3.org/ns/td#DataSchema">
        <rdfs:subClassOf>
